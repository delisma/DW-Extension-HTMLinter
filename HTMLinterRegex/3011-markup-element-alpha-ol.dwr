<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="((\s*&lt;p&gt;((\()?)\w\)\s*.+&lt;/p&gt;)+)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="\n<ol type=&quot;a&quot;>$1\n</ol>" param2=""/>
</dwquery>
